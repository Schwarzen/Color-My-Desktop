[Desktop Entry]
Name=Color My Gnome
Comment=Personalize your GNOME desktop colors
# In Flatpak, Exec must point to the 'command' defined in your YAML
Exec=color-my-gnome
Icon=org.gnome.ColorMyGnome
Terminal=false
Type=Application
Categories=GNOME;GTK;Settings;
StartupNotify=true
# This ID must match your App ID exactly
X-Flatpak=org.gnome.ColorMyGnome
